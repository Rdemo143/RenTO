{
  "routes": [
    { "handle": "filesystem" },
    { "src": "/manifest.json", "headers": { "content-type": "application/manifest+json" } },
    { "src": ".*\\.js$", "headers": { "content-type": "application/javascript" } },
    { "src": ".*\\.jsx$", "headers": { "content-type": "application/javascript" } },
    { "src": ".*\\.mjs$", "headers": { "content-type": "application/javascript" } },
    { "src": ".*\\.css$", "headers": { "content-type": "text/css" } },
    { "src": ".*\\.(png|jpg|jpeg|gif|webp|svg|ico)$", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 